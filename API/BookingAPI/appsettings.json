{
  "Database": {
    "Name": "{DATABASE_NAME}",
    "Host": "{DATABASE_HOST}",
    "Port": "{DATABASE_PORT}",
    "Login": "{DATABASE_LOGIN}",
    "Password": "{DATABASE_PASSWORD}"
  },

  "PasswordHashSecret": "{PASSWORD_HASH_SECRET}",

  "Identity": {
    "Host": "{IDENTITY_HOST}",
    "ValidAudience": "{IDENTITY_VALID_AUDIENCE}",
    "Secret": "{IDENTITY_SECRET}"
  },

  "Supabase": {
    "Url": "{SUPABASE_URL}",
    "Key": "{SUPABASE_KEY}"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
